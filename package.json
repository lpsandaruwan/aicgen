{
  "name": "aicgen",
  "version": "0.1.0",
  "description": "AI Config Generator - Automated AI assistant configuration for your projects",
  "keywords": [
    "ai",
    "cli",
    "code-generator",
    "claude",
    "config",
    "assistant"
  ],
  "author": "Lahiru Sandaruwan <lpsandaruwan@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lpsandaruwan/aicgen.git"
  },
  "bin": {
    "aicgen": "./dist/index.js"
  },
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "embed": "bun run scripts/embed-data.ts",
    "prebuild": "bun run embed",
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:binary": "bun run embed && bun run scripts/build-binary.ts current",
    "build:binary:windows": "bun run embed && bun run scripts/build-binary.ts windows",
    "build:binary:linux": "bun run embed && bun run scripts/build-binary.ts linux",
    "build:binary:macos": "bun run embed && bun run scripts/build-binary.ts macos",
    "build:all": "bun run build:binary:windows && bun run build:binary:linux && bun run build:binary:macos",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.65",
    "@anthropic-ai/sdk": "^0.30.1",
    "@google/generative-ai": "^0.24.1",
    "@inquirer/checkbox": "^5.0.2",
    "@inquirer/prompts": "^6.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "debug": "^4.3.7",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "openai": "^6.10.0",
    "ora": "^8.1.1",
    "winston": "^3.19.0",
    "yaml": "^2.6.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "^1.1.13",
    "@types/debug": "^4.1.12",
    "@types/decompress": "^4.2.7",
    "@types/node": "^20.17.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}